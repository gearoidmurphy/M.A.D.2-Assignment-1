<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/list">
    <fragment
        android:id="@+id/list"
        android:name="ie.wit.treatment.fragments.List"
        android:label="fragment_list"
        tools:layout="@layout/fragment_list" >
        <action
            android:id="@+id/action_list_to_addTreatment"
            app:destination="@id/addTreatment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/addTreatment"
        android:name="ie.wit.treatment.fragments.AddTreatment"
        android:label="fragment_add_treatment"
        tools:layout="@layout/fragment_add_treatment" >
        <action
            android:id="@+id/action_addTreatment_to_list"
            app:destination="@id/list"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
    </fragment>
</navigation>